{
  "$schema": "https://raw.githubusercontent.com/wraith13/vscode-schemas/master/en/latest/schemas/color-theme.json",
  "name": "Modified Darcula",
  "type": "dark",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    // General
    "enumMember": {
      "foreground": "#9876aa",
      "bold": true
    },
    "field": {
      "foreground": "#9876aa",
      "italic": true
    },
    "function.builtin": {
      "foreground": "#8888c6"
    },
    "namespace": {
      "foreground": "#a9b7c677",
      "italic": true
    },
    "property": {
      "foreground": "#9876aa",
      "italic": true
    },
    "type": {
      "foreground": "#b9bcd1",
      "italic": true
    },
    "type.defaultLibrary": {
      "foreground": "#cc7832",
      "italic": false
    },
    "type.deduced": {
      "foreground": "#cc7832",
      "italic": false
    },
    "typeParameter.readonly": {
      "foreground": "#9876aa",
      "italic": true
    },
    "variable.defaultLibrary": {
      "foreground": "#a9b7c6"
    },
    "variable.global": {
      "foreground": "#9876aa",
      "underline": true
    },
    "variable.readonly": {
      "foreground": "#9876aa",
      "bold": true
    },
    // Python
    "class.builtin:python": {
      "foreground": "#8888c6"
    }
  },
  "tokenColors": [
    // General
    {
      "name": "Attributes",
      "scope": [
        "support.other.attribute",
        "punctuation.section.attribute.begin",
        "punctuation.section.attribute.end"
      ],
      "settings": {
        "foreground": "#368a8a"
      }
    },
    {
      "name": "Block",
      "scope": [
        "punctuation.section.block.begin.bracket.curly",
        "punctuation.section.block.end.bracket.curly",
        "punctuation.terminator.statement",
        "punctuation.section.parens.begin.bracket.round",
        "punctuation.section.parens.end.bracket.round",
        "punctuation.section.parameters.begin.bracket.round",
        "punctuation.section.parameters.end.bracket.round"
      ],
      "settings": {
        "foreground": "#a9b7c6"
      }
    },
    {
      "name": "Casts",
      "scope": ["keyword.operator.cast"],
      "settings": {
        "foreground": "#629755",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Class",
      "scope": ["entity.name.type.class"],
      "settings": {
        "foreground": "#a9b7c6"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#808080"
      }
    },
    {
      "name": "Comment - Documentation",
      "scope": [
        "comment.documentation",
        "comment.block.documentation",
        "punctuation.definition.comment.begin.documentation",
        "punctuation.definition.comment.end.documentation"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#629755"
      }
    },
    {
      "name": "Constants",
      "scope": ["constant"],
      "settings": {
        "foreground": "#9876aa"
      }
    },
    {
      "name": "Directives & Preprocessor",
      "scope": [
        "keyword.control.directive",
        "keyword.preprocessor",
        "entity.name.function.decorator",
        "meta.preprocessor",
        "meta.preprocessor.cs"
      ],
      "settings": {
        "foreground": "#bbb529"
      }
    },
    {
      "name": "Documentation Names",
      "scope": ["comment.documentation.name"],
      "settings": {
        "foreground": "#cc7832",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Entity Names",
      "scope": ["entity.name"],
      "settings": {
        "foreground": "#a9b7c6"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#cc7832"
      }
    },
    {
      "name": "Event",
      "scope": ["variable.other.event", "support.class.event", "support.class.builtin.event"],
      "settings": {
        "foreground": "#ffc66d",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Field",
      "scope": [
        "entity.name.variable.field",
      ],
      "settings": {
        "foreground": "#9876aa",
        "fontStyle": "italic",
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.command",
        "entity.name.function",
        "meta.function-call",
        "meta.require",
        "support.function",
        "variable.function"
      ],
      "settings": {
        "foreground": "#ffc66d"
      }
    },
    {
      "name": "Function",
      "scope": ["meta.function"],
      "settings": {
        "foreground": "#a9b7c6"
      }
    },
    {
      "name": "Function Parameter",
      "scope": ["variable.parameter.function"],
      "settings": {
        "foreground": "#a9b7c6"
      }
    },
    {
      "name": "Function - Built-in",
      "scope": ["support.function.builtin"],
      "settings": {
        "foreground": "#8888c6"
      }
    },
    {
      "name": "Integers",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#6897bb"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "constant.language",
        "constant.numeric.other.preffix",
        "constant.numeric.other.suffix",
        "keyword",
        "keyword.operator.sizeof",
        "keyword.operator.wordlike",
        "keyword.operator.new",
        "punctuation.definition.interpolation",
        "storage.modifier",
      ],
      "settings": {
        "foreground": "#cc7832"
      }
    },
    {
      "name": "Line Terminator",
      "scope": ["punctuation.terminator.statement"],
      "settings": {
        "foreground": "#cc7832"
      }
    },
    {
      "name": "Namespace Selector",
      "scope": [
        "punctuation.separator.scope-resolution",
        "entity.name.scope-resolution"
      ],
      "settings": {
        "foreground": "#a9b7c677",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#a9b7c6"
      }
    },
    {
      "name": "Separators",
      "scope": ["punctuation.separator"],
      "settings": {
        "foreground": "#cc7832"
      }
    },
    {
      "name": "Strings",
      "scope": ["string"],
      "settings": {
        "foreground": "#6a8759"
      }
    },
    {
      "name": "String interpolation",
      "scope": [
        "punctuation.definition.template-expression",
        "punctuation.section.embedded",
        "variable.interpolation"
      ],
      "settings": {
        "foreground": "#cc7832"
      }
    },
    {
      "name": "Support Type",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#8888c6"
      }
    },
    {
      "name": "Template entity",
      "scope": ["entity.name.type.template", "entity.name.type.parameter"],
      "settings": {
        "foreground": "#9876aa"
      }
    },
    {
      "name": "'This'",
      "scope": [
        "variable.language.this",
        "variable.parameter.function.language.special.self",
        "variable.language.special.self"
      ],
      "settings": {
        "foreground": "#cc7832"
      }
    },
    {
      "name": "Types",
      "scope": ["storage.type"],
      "settings": {
        "foreground": "#cc7832"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable"],
      "settings": {
        "foreground": "#a9b7c6"
      }
    },
    {
      "name": "Variables - Function Calls",
      "scope": ["variable.parameter.function-call"],
      "settings": {
        "foreground": "#aa4926"
      }
    },
    // C/C++
    {
      "name": "C++ static_assert",
      "scope": ["keyword.other.static_assert"],
      "settings": {
        "foreground": "#629755",
        "fontStyle": "italic"
      }
    },
    // C#
    {
      "name": "C# Comment attributes",
      "scope": ["comment.documentation.attribute.name.cs"],
      "settings": {
        "foreground": "#bababa",
        "fontStyle": "italic"
      }
    },
    {
      "name": "C# Comment tags",
      "scope": [
        "entity.name.tag.localname.cs",
        "punctuation.definition.tag.cs"
      ],
      "settings": {
        "foreground": "#cc7832",
        "fontStyle": "italic"
      }
    },
    {
      "name": "C# Directives & preprocessor",
      "scope": [
        "meta.preprocessor.cs", 
        "punctuation.separator.hash.cs",
      ],
      "settings": {
        "foreground": "#bbb529"
      }
    },

    // CMake
    {
      "name": "CMake Entity",
      "scope": ["entity"],
      "settings": {
        "foreground": "#cc7832"
      }
    },
    {
      "name": "CMake Keyword",
      "scope": ["keyword.cmake"],
      "settings": {
        "foreground": "#ffc66d"
      }
    },
    // Git
    {
      "name": "Git ignore",
      "scope": ["keyword.file.content.ignore"],
      "settings": {
        "foreground": "#6a8759"
      }
    },
    {
      "name": "Git message line too long",
      "scope": ["invalid.deprecated.line-too-long.git-commit"],
      "settings": {
        "foreground": "#f44747"
      }
    },
    // HTML
    {
      "name": "HTML attribute name",
      "scope": [
        "entity.other.attribute-name",
        "punctuation.separator.namespace.html"
      ],
      "settings": {
        "foreground": "#bababa"
      }
    },
    {
      "name": "HTML data",
      "scope": ["string.unquoted.cdata.html"],
      "settings": {
        "foreground": "#a9b7c6"
      }
    },
    {
      "name": "HTML entity",
      "scope": ["constant.character.entity"],
      "settings": {
        "foreground": "#6897bb"
      }
    },
    {
      "name": "HTML text",
      "scope": ["text.html"],
      "settings": {
        "foreground": "#a9b7c6"
      }
    },
    {
      "name": "HTML values",
      "scope": [
        "punctuation.definition.string.begin.html",
        "punctuation.definition.string.end.html",
        "string.quoted.double.html",
        "string.quoted.single.html",
        "string.unquoted.html"
      ],
      "settings": {
        "foreground": "#A5C261"
      }
    },
    {
      "name": "HTML tag",
      "scope": [
        "entity.name.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#e8bf6a"
      }
    },
    // JSON
    {
      "name": "JSON Bad escapes",
      "scope": ["invalid.illegal.unrecognized-string-escape.json"],
      "settings": {
        "foreground": "#f44747",
        "fontStyle": "underline"
      }
    },
    {
      "name": "JSON Key",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#9876aa"
      }
    },
    {
      "name": "JSON Punctuation",
      "scope": [
        "punctuation.separator.dictionary.key-value.json",
        "punctuation.separator.dictionary.pair.json",
        "punctuation.separator.array.json"
      ],
      "settings": {
        "foreground": "#cc7832"
      }
    },
    // Markdown
    {
      "name": "Markdown Code",
      "scope": [
        "markup.fenced_code.block.markdown",
        "markup.inline.raw.string.markdown",
        "markup.raw.block.markdown"
      ],
      "settings": {
        "foreground": "#6a8759"
      }
    },
    {
      "name": "Markdown Link",
      "scope": [
        "markup.underline.link.markdown",
        "punctuation.definition.link.markdown",
        "string.other.link.title.markdown",
        "meta.image.inline.markdown",
        "string.other.link.description.markdown",
        "meta.link.inline.markdown",
        "meta.link.reference.markdown"
      ],
      "settings": {
        "foreground": "#287bde"
      }
    },
    {
      "name": "Markdown Link Label",
      "scope": [
        "constant.other.reference.link.markdown",
        "punctuation.definition.constant.begin.markdown",
        "punctuation.definition.constant.end.markdown",
        "punctuation.definition.constant.markdown"
      ],
      "settings": {
        "foreground": "#cc7832"
      }
    },
    {
      "name": "Markdown Headers",
      "scope": [
        "entity.name.section.markdown",
        "punctuation.definition.heading.markdown",
        "markup.heading.setext.1.markdown",
        "markup.heading.setext.2.markdown",
        "markup.heading.setext.3.markdown",
        "markup.heading.setext.4.markdown",
        "markup.heading.setext.5.markdown",
        "markup.heading.setext.6.markdown"
      ],
      "settings": {
        "foreground": "#9876aa",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": ["markup.bold.markdown"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Style Delineators",
      "scope": [
        "punctuation.definition.bold.markdown",
        "punctuation.definition.italic.markdown",
        "punctuation.definition.strikethrough.markdown",
        "punctuation.definition.underline.markdown"
      ],
      "settings": {
        "foreground": "#cc7832"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": ["markup.italic.markdown"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Separators",
      "scope": ["meta.separator.markdown"],
      "settings": {
        "foreground": "#cc7832"
      }
    },
    {
      "name": "Markdown Strikethrough",
      "scope": ["markup.strikethrough.italic"],
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "Markdown Underline",
      "scope": ["markup.underline.markdown"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markdown Quotes",
      "scope": ["markup.quote.markdown"],
      "settings": {
        "foreground": "#6a8759"
      }
    },

    // Python
    {
      "name": "Python Bytes",
      "scope": ["string.quoted.binary"],
      "settings": {
        "foreground": "#a5c261"
      }
    },
    {
      "name": "Python Magic",
      "scope": ["support.variable.magic", "support.function.magic"],
      "settings": {
        "foreground": "#b200b2"
      }
    },
    {
      "name": "Python Self",
      "scope": [
        "variable.language.special.self.python",
        "variable.parameter.function.language.special.self.python"
      ],
      "settings": {
        "foreground": "#94558d"
      }
    },

    // XML
    {
      "name": "XML attribute name",
      "scope": [
        "entity.other.attribute-name",
        "entity.name.tag.xml",
        "punctuation.separator.namespace.xml"
      ],
      "settings": {
        "foreground": "#bababa"
      }
    },
    {
      "name": "XML data",
      "scope": ["string.unquoted.cdata.xml"],
      "settings": {
        "foreground": "#a9b7c6"
      }
    },
    {
      "name": "XML entity",
      "scope": ["constant.character.entity.xml"],
      "settings": {
        "foreground": "#6897bb"
      }
    },
    {
      "name": "XML text",
      "scope": ["text.xml"],
      "settings": {
        "foreground": "#a9b7c6"
      }
    },
    {
      "name": "XML namespace",
      "scope": [
        "entity.name.tag.namespace.xml",
        "entity.other.attribute-name.namespace.xml"
      ],
      "settings": {
        "foreground": "#9876aa"
      }
    },
    {
      "name": "XML string",
      "scope": [
        "punctuation.definition.string.begin.xml",
        "punctuation.definition.string.end.xml"
      ],
      "settings": {
        "foreground": "#6A8759"
      }
    },
    {
      "name": "XML tag",
      "scope": ["punctuation.definition.tag.xml", "keyword.other.doctype.xml"],
      "settings": {
        "foreground": "#e8bf6a"
      }
    },
    {
      "name": "XML tag name",
      "scope": ["entity.name.tag.localname.xml"],
      "settings": {
        "foreground": "#e8bf6a"
      }
    },

    // Other
    {
      "name": "Selector",
      "scope": ["meta.selector"],
      "settings": {
        "foreground": "#cc7832"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#a9b7c6"
      }
    },
    {
      "name": "Embedded",
      "scope": ["punctuation.section.embedded"],
      "settings": {
        "foreground": "#cc7832"
      }
    },
    {
      "name": "illegal",
      "scope": ["invalid.illegal"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Broken",
      "scope": ["invalid.broken"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Deprecated",
      "scope": ["invalid.deprecated"],
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "Unimplemented",
      "scope": ["invalid.unimplemented"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "scope": ["token.info-token"],
      "settings": {
        "foreground": "#ffc66d"
      }
    },
    {
      "scope": ["token.warn-token"],
      "settings": {
        "foreground": "#6897bb"
      }
    },
    {
      "scope": ["token.error-token"],
      "settings": {
        "foreground": "#f44747"
      }
    },
    {
      "scope": ["token.debug-token"],
      "settings": {
        "foreground": "#cc7832"
      }
    }
  ],
